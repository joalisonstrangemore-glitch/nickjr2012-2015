<?xml version="1.0" encoding="UTF-8"?>
<Settings>
  <!-- relative paths -->
  <!--
  <SwfsToLoad>
    <SwfToLoad id="music" path="/game/assets/dora_valentine/music/VinesMusic.swf" target="none" />
  </SwfsToLoad>
  -->

  <!-- absolute paths -->
  <SwfsToLoad>
    <SwfToLoad id="music" path="/game/assets/dora_valentine/music/VinesMusic.swf" target="none" />
  </SwfsToLoad>

  <!-- show the debug tracer window -->
  <ShowTracer>false</ShowTracer> 
  
  <Debug>false</Debug>

  <!-- turn off custom trace messages -->
  <Quiet>true</Quiet> 
  
  <!-- Colors -->
  <Colors>
    <Color name="red" sound="DLV_23.mp3" altSound="DLV_23.mp3" />
    <Color name="blue" sound="DLV_24.mp3" altSound="DLV_24.mp3" />
    <Color name="yellow" sound="DLV_25.mp3" altSound="DLV_25.mp3" />
    <Color name="white" sound="DLV_26.mp3" altSound="DLV_26.mp3" />
    <!-- <Color name="pink" sound="DLV_27.mp3" altSound="DLV_27.mp3" /> -->
    <!-- <Color name="purple" sound="DLV_28.mp3" altSound="DLV_28.mp3" /> -->
    <!-- <Color name="orange" sound="DLV_29.mp3" altSound="DLV_29.mp3" /> -->
    <Color name="green" sound="DLV_30.mp3" altSound="DLV_30.mp3" />
    <!-- <Color name="black" sound="TMB_105.mp3" /> -->
  </Colors>
  
  <!-- Problem sets -->
  <!-- Patterns with 2, 3, and 4 vines are supported -->
  <!-- There should be no more than three unique colors per pattern (not enforced) -->
  <!-- Use "v<n>" attributes for the vine colors.  
       Use "missing" attribute with a number 1-n to force the missing vine for a set-->
  <ProblemSets>

    <!-- <VinePattern set="1" v1="red" v2="yellow" /> -->
    <VinePattern set="1" v1="blue" v2="white" audioBase="DLV_55.mp3" />
    <VinePattern set="1" v1="yellow" v2="blue" audioBase="DLV_56.mp3" />

    <VinePattern set="2" v1="red" v2="blue" v3="yellow" missing="3" audioBase="DLV_57.mp3" />
    <VinePattern set="2" v1="yellow" v2="blue" v3="white" missing="2" audioBase="DLV_58.mp3" />
    <VinePattern set="2" v1="blue" v2="yellow" v3="red" missing="3" audioBase="DLV_59.mp3" />

    <VinePattern set="3" v1="white" v2="white" v3="blue" missing="2" audioBase="DLV_60.mp3" />
    <VinePattern set="3" v1="yellow" v2="blue" v3="yellow" missing="3" audioBase="DLV_61.mp3" />
    <VinePattern set="3" v1="yellow" v2="red" v3="red" missing="2" audioBase="DLV_62.mp3" />
    
    <!--
    <VinePattern v1="red" v2="green" />
    <VinePattern v1="blue" v2="yellow" />
    <VinePattern v1="orange" v2="black" missing="1"/>
    <VinePattern v1="black" v2="white" v3="pink" />
    <VinePattern v1="pink" v2="green" v3="purple" />
    <VinePattern v1="blue" v2="blue" v3="pink" missing="2" />
    <VinePattern v1="red" v2="orange" v3="yellow" v4="red" missing="4"/>
    <VinePattern v1="green" v2="purple" v3="green" v4="orange" />
    <VinePattern v1="orange" v2="green" v3="yellow" v4="yellow" />
    -->
  </ProblemSets>
  
  <!-- Problem set order -->
  <!-- This describes how many problems are presented, 
       and which problem group (set) they come from -->
  <ProblemSetOrder>1, 2, 3</ProblemSetOrder>

