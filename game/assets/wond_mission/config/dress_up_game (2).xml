	<display_name>Wonderpets Dress Up Game</display_name>
	<version>0.001</version>
	<quality>MEDIUM</quality>	<!-- LOW MEDIUM HIGH -->
	<scale_mode>noScale</scale_mode>  <!-- noScale showAll -->
	<allowedInactivityTime>12000</allowedInactivityTime> <!-- milliseconds -->
	<pointTarget>1</pointTarget> <!-- milliseconds -->
	 <!-- Audio settings -->
	<audio>
		<!-- audioChannels, dynamic movieclips that sound objects attached to -->
		<!-- flash8 can support up to 32 simultaneous audio channels -->
		<audioChannels>
			<audioChannel id="VO"/>
			<audioChannel id="SFX"/>
			<audioChannel id="BG_MUSIC"/>
			<audioChannel id="MISC"/>
			<audioChannel id="MISC2"/>
		</audioChannels>
		<!-- audioChannels, dynamic movieclips that sound objects attached to -->
		<audioItems>
			<audioItem id="wp_animalscreen_m.aif" loc="embedded" type="vo" channelID="VO" handle="audioInstructionVoID" callback="onCompleteInstructionSequence" offset="0" loops="0" duration="100%"/>
			<audioItem id="wp_instruct_music_end01.aif" loc="embedded" type="sfx"  channelID="SFX" handle="audioInterstitialMusicID" callback="onCompleteInterstitialMusic" offset="0" loops="0" duration="100%"/>
			<audioItem id="dressupgameloop01.aif" loc="embedded" type="bg_music"  channelID="BG_MUSIC" handle="audioBackgroundMusicID" callback="null" offset="0" loops="500" duration="100%"/>
			<audioItem id="beach music end.aif" loc="embedded" type="bg_music"  channelID="BG_MUSIC" handle="audioBackgroundMusicEndID" callback="null" offset="0" loops="0" duration="100%"/>	
