			<audioItem id="select_04.aif" loc="embedded" type="sfx"  channelID="SFX" handle="audioSfxOnDragClothingItem" callback="null" offset="0" loops="0" duration="100%"/>			
			<audioItem id="Place_01.aif" loc="embedded" type="sfx"  channelID="SFX" handle="audioSfxOnDropClothingItem" callback="null" offset="0" loops="0" duration="100%"/>		
			<audioItem id="vo_pickoutfit_01.aif" loc="embedded" type="vo"  channelID="MISC" handle="audioVoInactivityPrompt" callback="null" offset="0" loops="0" duration="100%"/>		
			<audioItem id="select_04.aif" loc="embedded" type="sfx"  channelID="SFX" handle="audioSfxOnButtonClick" callback="null" offset="0" loops="0" duration="100%"/>		
		</audioItems>
	</audio>
	
	
